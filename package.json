{
  "name": "wci-build",
  "version": "1.2.2",
  "description": "A webpack scaffold",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib"
  },
  "bin": {
    "wci-build": "./bin/wci-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jason-live/wci-build.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime"
    ]
  },
  "keywords": [
    "build",
    "webpack",
    "react 16",
    "rudex",
    "react-router 4"
  ],
  "author": "jasonlive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jason-live/wci-build/issues"
  },
  "homepage": "https://github.com/jason-live/wci-build#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.3.0",
    "commander": "^2.13.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^7.2.0",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.9",
    "shelljs": "^0.8.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "which": "^1.3.0",
    "win-spawn": "^2.0.0"
  }
}
